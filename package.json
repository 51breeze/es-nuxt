{
  "name": "es-nuxt",
  "version": "0.0.10",
  "typings": "types/typings.json",
  "esconfig": {
    "scope": "es-nuxt",
    "inherits": [
      "es-vue",
      "es-javascript"
    ]
  },
  "scripts": {
    "dev-test": "jasmine ./test/index.js",
    "manifest":"node ./scripts/manifest.js",
    "build": "cd test && npm run build",
    "dev": "cd test && npm run dev",
    "generate": "cd test && npm run generate",
    "preview": "cd test && npm run preview",
    "postinstall": "cd test && npm run prepare"
  },
  "dependencies": {
    "es-vue": "latest"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.7.3",
    "easescript": "latest",
    "es-vite-plugin": "latest",
    "lodash": "^4.17.21"
  }
}
